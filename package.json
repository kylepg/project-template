{
  "name": "project-name",
  "version": "4.0.0",
  "description": "Project",
  "author": "Kyle George",
  "license": "MIT",
  "config": {
    "cdn": {
      "do": "s3://celticsdigital/<project-path>",
      "nba": "s3://nba-static-celtics-prod/cdn/<project-path>"
    },
    "s3cmd": {
      "do": "/Users/kyle/.s3cfg_do",
      "nba": "/Users/kyle/.s3cfg_nba"
    },
    "name": "project-name",
    "path": "<project-path>",
    "version": "1.0"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "scripts": {
    "init": "npm run build && git checkout -b dev && npm rebuild node-sass",
    "sync": "browser-sync start --proxy 'localhost:1234' -w",
    "watch": "parcel ./src/index.html --out-dir dist/dev/ --port 3434 & onchange 'src/html/*' 'src/html' -- touch ./src/index.html",
    "build": "rm -f dist/*.js && rm -f dist/*.css && rm -f dist/*.css && rm -f dist/*.css && parcel build  ./src/index.html --no-source-maps",
    "build-main": "parcel build  ./src/html/main.html --no-source-maps",
    "git_push": "git push && git checkout master && git push && git checkout dev",
    "git_master-commit": "TS=`date +%Y-%m-%d__%H:%M:%S`; git add -A && git commit -m 'Publish commit: '$TS && git checkout master && git merge dev && git checkout dev",
    "stage": "npm run publish --append='_stage'",
    "publish": "npm run build && npm run publish_nba && npm run publish_do && npm run git_master-commit",
    "publish_js": "npm run publish_nba_js && npm run publish_do_js",
    "publish_css": "npm run publish_nba_css && npm run publish_do_css",
    "publish_config": "npm run publish_nba_config && npm run publish_do_config",
    "publish_data": "npm run publish_nba_data && npm run publish_do_data",
    "publish_misc": "npm run publish_nba_misc && npm run publish_do_misc",
    "publish_images": "npm run publish_nba_images && npm run publish_do_images && mv -f ./src/assets/images/queue/* ./src/assets/images",
    "publish_videos": "npm run publish_nba_videos && npm run publish_do_videos",
    "publish_nba": "npm run publish_nba_js && npm run publish_nba_css",
    "publish_nba_js": "s3cmd -c $npm_package_config_s3cmd_nba put ./dist/$(find . -maxdepth 2 -name '*main*.js' -type f -exec basename {} \\; -quit;) $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/$(echo $npm_package_config_name)$(echo $npm_config_append).min.js --no-mime-magic --acl-public --add-header='Cache-Control:no-cache'",
    "publish_nba_css": "cat ./dist/*.css > ./dist/upload.css && s3cmd -c $npm_package_config_s3cmd_nba put ./dist/$(find . -maxdepth 2 -name 'upload.css' -type f -exec basename {} \\; -quit;) $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/$(echo $npm_package_config_name)$(echo $npm_config_append).min.css --no-mime-magic --acl-public --add-header='Cache-Control:no-cache'",
    "publish_nba_config": "s3cmd -c $npm_package_config_s3cmd_nba put ./src/config/config.json $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/config.json --no-mime-magic --acl-public --add-header='Cache-Control:no-cache'",
    "publish_nba_data": "s3cmd -c $npm_package_config_s3cmd_nba put ./src/data/* $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/data/ --no-mime-magic --acl-public --recursive",
    "publish_nba_misc": "s3cmd -c $npm_package_config_s3cmd_nba put ./src/assets/misc/* $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/misc/ --no-mime-magic --acl-public --recursive",
    "publish_nba_images": "s3cmd -c $npm_package_config_s3cmd_nba put ./src/assets/images/queue/* $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/images/ --no-mime-magic --acl-public --recursive",
    "publish_nba_videos": "s3cmd -c $npm_package_config_s3cmd_nba put ./src/assets/videos/* $(echo $npm_package_config_cdn_nba)/$(echo $npm_package_config_version)/videos/ --no-mime-magic --acl-public --recursive",
    "publish_do": "npm run publish_do_js && npm run publish_do_css",
    "publish_do_js": "s3cmd -c $npm_package_config_s3cmd_do put ./dist/$(find . -maxdepth 2 -name '*main*.js' -type f -exec basename {} \\; -quit;) $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/$(echo $npm_package_config_name)$(echo $npm_config_append).min.js --no-mime-magic --acl-public",
    "publish_do_css": "s3cmd -c $npm_package_config_s3cmd_do put ./dist/$(find . -maxdepth 2 -name 'upload.css' -type f -exec basename {} \\; -quit;) $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/$(echo $npm_package_config_name)$(echo $npm_config_append).min.css --no-mime-magic --acl-public",
    "publish_do_config": "s3cmd -c $npm_package_config_s3cmd_do put ./src/config/config.json $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/config.json --no-mime-magic --acl-public",
    "publish_do_data": "s3cmd -c $npm_package_config_s3cmd_do put ./src/data/* $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/data/ --no-mime-magic --acl-public --recursive",
    "publish_do_misc": "s3cmd -c $npm_package_config_s3cmd_do put ./src/assets/misc/* $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/misc/ --no-mime-magic --acl-public --recursive",
    "publish_do_images": "s3cmd -c $npm_package_config_s3cmd_do put ./src/assets/images/queue/* $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/images/ --no-mime-magic --acl-public --recursive",
    "publish_do_videos": "s3cmd -c $npm_package_config_s3cmd_do put ./src/assets/videos/* $(echo $npm_package_config_cdn_do)/$(echo $npm_package_config_version)/videos/ --no-mime-magic --acl-public --recursive"
  },
  "assetsPath": "src/assets",
  "eslintIgnore": [
    "index.js",
    "dist/**/*.js",
    "dist/*.js",
    "node_modules/"
  ],
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:vue/recommended"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "no-new": 0,
      "eqeqeq": 0,
      "func-names": 0,
      "global-require": 0,
      "ignoreStrings": true,
      "import/extensions": 0,
      "no-console": 0,
      "no-global-assign": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "no-var": 0,
      "vue/html-self-closing": 0,
      "vue/require-default-prop": 0,
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 3,
          "multiline": {
            "max": 3,
            "allowFirstLine": true
          }
        }
      ],
      "prefer-destructuring": 0,
      "quotes": [
        "error",
        "backtick"
      ],
      "space-before-function-paren": 0,
      "spaced-comment": 0,
      "wrap-iife": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "babel": {
    "sourceMap": "inline",
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ],
    "minified": true
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.1",
    "autoprefixer": "^9.7.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "core-js": "^3.6.4",
    "cssnano": "^4.1.10",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.17.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "onchange": "^5.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-asset-copier": "^1.0.0",
    "posthtml": "^0.11.6",
    "posthtml-alt-always": "^1.0.1",
    "posthtml-custom-elements": "^1.1.2",
    "posthtml-minifier": "^0.1.0",
    "posthtml-modules": "^0.4.2",
    "posthtml-tidy": "^0.8.0",
    "posthtml-w3c": "^0.8.2",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "uglifyify": "^4.0.5",
    "eslint-plugin-vue": "^6.2.2",
    "vue-eslint-parser": "^7.0.0",
    "vue-hot-reload-api": "^2.3.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "tailwindcss": "^1.2.0",
    "vue": "^2.6.11"
  }
}
